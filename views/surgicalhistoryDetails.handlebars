        <!-- page content -->
        <div class="right_col" role="main">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3>Surgical History </h3>
              </div>

              <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                      <button class="btn btn-default" type="button">Go!</button>
                    </span>
                  </div>
                </div>
              </div>
            </div>

            <div class="clearfix"></div>

            <div class="row">

              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>List of Surgeries</h2>
                    <div class="clearfix"></div>
                  </div>

                  <div class="x_content">

                    <div class="table-responsive">
                      <table class="table table-striped jambo_table">
                        <thead>
                          <tr class="headings">
                            <th class="column-title">Procedure Type </th>
                            <th class="column-title">Date </th>
                            <th class="column-title">Hospital Name </th>
                            <th class="column-title">Body Location </th>
                            <th class="column-title">Operated By </th>
                            <th class="column-title">Surgical Notes </th>
                            <th class="column-title">Physician Notes </th>
                            <th class="column-title">Anesthesia Notes </th>
                            <th class="column-title">Consequences </th>
                        
                          </tr>
                        </thead>

                        <tbody>
                                            
                            {{#each patient}}
                            {{#each surgicalHistory}}
                            <tr class="even pointer">
                            <td class=" ">{{this.procedureType}}</td>
                            <td class=" ">{{this.date}} </td>
                            <td class=" ">{{this.hospital}}</td>
                            <td class=" ">{{this.bodyLocation}}</td>
                            <td class=" ">{{this.operatedBy}}</td>
                            <td class=" ">{{this.surgicalNotes}}</td>
                            <td class=" ">{{this.physicianNotes}}</td>
                            <td class=" ">{{this.anesthesiaNotes}}</td>
                            <td class=" ">{{this.consequence}}</td>
                          </tr>

                          {{/each}}
                         {{/each}}
                          
                          
                        </tbody>
                      </table>
                    </div>
							
						
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /page content -->