        <!-- page content -->
        <div class="right_col" role="main">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3>Medications </h3>
              </div>

              <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                      <button class="btn btn-default" type="button">Go!</button>
                    </span>
                  </div>
                </div>
              </div>
            </div>

            <div class="clearfix"></div>

            <div class="row">

              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>List of Medications</h2>
                    <div class="clearfix"></div>
                  </div>

                  <div class="x_content">

                    <div class="table-responsive">
                      <table class="table table-striped jambo_table">
                        <thead>
                          <tr class="headings">
                            <th class="column-title">Name </th>
                            <th class="column-title">Dose </th>
                            <th class="column-title">Frequency </th>
                            <th class="column-title">Time Interval </th>
                            <th class="column-title">Strength </th>
                            <th class="column-title">Medication Type </th>
                            <th class="column-title">Prescribed By </th>
                            <th class="column-title">Instructions </th>
                            <th class="column-title">Reason For Taking </th>
                            <th class="column-title">Start Date </th>
                            <th class="column-title">End Date </th>
                            <th class="column-title">Currently Taking </th>
                            <th class="column-title">Notes </th>
                        
                          </tr>
                        </thead>

                        <tbody>
                                            
                            {{#each patient}}
                            {{#each medications}}
                            <tr class="even pointer">
                            <td class=" ">{{this.name}}</td>
                            <td class=" ">{{this.dose}} </td>
                            <td class=" ">{{this.frequency}}</td>
                            <td class=" ">{{this.timeInterval}}</td>
                            <td class=" ">{{this.strength}}</td>
                            <td class=" ">{{this.medicationType}}</td>
                            <td class=" ">{{this.prescribedBy}}</td>
                            <td class=" ">{{this.instructions}} </td>
                            <td class=" ">{{this.reasonsForTaking}}</td>
                            <td class=" ">{{this.startDate}}</td>
                            <td class=" ">{{this.endDate}}</td>
                            <td class=" ">{{this.currentlyTaking}}</td>
                            <td class=" ">{{this.notes}}</td>
                          </tr>

                          {{/each}}
                         {{/each}}
                          
                          
                        </tbody>
                      </table>
                    </div>
							
						
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /page content -->