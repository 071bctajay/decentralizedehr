        <!-- page content -->
        <div class="right_col" role="main">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3>Vital Signs </h3>
              </div>

              <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                      <button class="btn btn-default" type="button">Go!</button>
                    </span>
                  </div>
                </div>
              </div>
            </div>

            <div class="clearfix"></div>

            <div class="row">

              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>List of Vital Signs</h2>
                    <div class="clearfix"></div>
                  </div>

                  <div class="x_content">

                    <div class="table-responsive">
                      <table class="table table-striped jambo_table">
                        <thead>
                          <tr class="headings">
                            <th class="column-title">Name </th>
                            <th class="column-title">Date </th>
                            <th class="column-title">Status </th>
                            <th class="column-title">View Detail </th>
                            <th class="column-title">Unit </th>
                            <th class="column-title">Notes </th>
                        
                          </tr>
                        </thead>

                        <tbody>
                                            
                            {{#each patient}}
                            {{#each vitalSign}}
                            <tr class="even pointer">
                            <td class=" ">{{this.name}}</td>
                            <td class=" ">{{this.dateOfNote}} </td>
                            <td class=" ">{{this.status}}</td>
                            <td class=" ">{{this.value}}</td>
                            <td class=" ">{{this.unit}}</td>
                            <td class=" ">{{this.notes}}</td>
                          </tr>

                          {{/each}}
                         {{/each}}
                          
                          
                        </tbody>
                      </table>
                    </div>
							
						
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /page content -->